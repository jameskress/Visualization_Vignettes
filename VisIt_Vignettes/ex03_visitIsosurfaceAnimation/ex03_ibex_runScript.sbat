# set script vars
_nodes=1
_ntasks=2
_time=00:05:00

# load the visit environment
source ../MODULES.sh

`(which visit)` -nowin -cli -s ex03_visitIsosurfaceAnimation.py $_nodes $_ntasks $_time "ibex"
