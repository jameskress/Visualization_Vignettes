#SBATCH -N 1
#SBATCH --ntasks-per-node=1
#SBATCH --partition=batch
#SBATCH -J ex00.ibex
#SBATCH -o ex00.ibex.%J.out
#SBATCH -e ex00.ibex.%J.out
#SBATCH --time=00:02:00

# set script vars
_nodes=1
_ntasks=1
_time=00:02:00

# load the visit environment
source ../MODULES.sh

time `(which visit)` -nowin -cli -s ex00_visitQuery.py $_nodes $_ntasks $_time "ibex"
